{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }]
    "builds": [
        {
            "src": "./index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ]

}




